<view><view class="all display_column"><view class="top display_row"><text data-event-opts="{{[['tap',[['index_1',['$event']]]]]}}" class="{{[index==1?'click_red':'']}}" bindtap="__e">全部</text><text data-event-opts="{{[['tap',[['index_2',['$event']]]]]}}" class="{{[index==2?'click_red':'']}}" bindtap="__e">交易成功</text><text data-event-opts="{{[['tap',[['index_3',['$event']]]]]}}" class="{{[index==3?'click_red':'']}}" bindtap="__e">交易失败</text><text data-event-opts="{{[['tap',[['index_4',['$event']]]]]}}" class="{{[index==4?'click_red':'']}}" bindtap="__e">待处理</text></view><view class="middle"><view hidden="{{!(isShow)}}" class="no display_column"><image src="../../static/images/tabbar/zanwushuju.png" mode></image><text class="ccc">暂无数据</text></view><view hidden="{{!(!isShow)}}" class="yes"></view></view></view></view>