<view><view class="all display_column"><view class="top display_row"><text data-event-opts="{{[['tap',[['index_1',['$event']]]]]}}" class="{{[index==1?'click_red':'']}}" bindtap="__e">加油卡</text><text data-event-opts="{{[['tap',[['index_2',['$event']]]]]}}" class="{{[index==2?'click_red':'']}}" bindtap="__e">话费卡</text><text data-event-opts="{{[['tap',[['index_3',['$event']]]]]}}" class="{{[index==3?'click_red':'']}}" bindtap="__e">游戏卡</text><text data-event-opts="{{[['tap',[['index_4',['$event']]]]]}}" class="{{[index==4?'click_red':'']}}" bindtap="__e">商超卡</text></view><view class="middle display_column"><block wx:for="{{wrapList_}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['go_card',['$event']]]]]}}" class="m_item display_row" bindtap="__e"><view class="m_item_1 display_row"><image class="imgs" src="{{item.url}}" mode></image><view class="m_item_text display_column"><text style="margin-bottom:20rpx;">{{item.name}}</text><text class="red">{{item.discount}}</text></view></view><image class="imgs_" src="../../static/images/tabbar/jiantou_.png" mode></image></view></block></view></view></view>