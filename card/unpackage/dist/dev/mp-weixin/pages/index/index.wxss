
uni-image {
	width: 100% !important;
}
.swiper-item {
	width: 100%;
}
.middle {
	position: relative;
	width: 100%;
}
.m_item {
	margin-top: 40rpx;
}
.m_item text:nth-child(2) {
	padding: 0 20rpx;
	font-size: 38rpx;
	font-weight: 600;
	letter-spacing: 2px;
	color: #666666;
}
.wrap {
	width: 100%;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}
.wrap view {
	width: 28%;
	margin: 5% 2%;
	color: #666666;
	font-size: 32rpx;
}
.wrap image {
	width: 120rpx !important;
	height: 120rpx;
	margin-bottom: 20rpx;
	border-radius: 50%;
}
.text_tail {
	margin-top: 20rpx;
	font-size: 40rpx;
	margin-bottom: 30rpx;
}
.tail_wrap {
	width: 100%;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	font-size: 32rpx;
}
.tail_item {
	width: 29%;
	margin: 2% 1%;
}
.tail_item image {
	width: 210rpx !important;
	height: 180rpx;
}
.position_ {
	position: fixed;
	width: 65rpx;
	height: 70rpx;
	top: 420rpx;
	right: 40rpx;
}
.position_ image {
	width: 100%;
	height: 100%;
}
.uni-padding-wrap {
	margin-top: 60rpx;
	display: table;
	vertical-align: middle;
	text-align: center;
	/* margin-right: 150upx; */
}
._position {
	position: absolute;
	top: -14rpx;
	left: 50rpx;
	width: 160rpx;
	height: 80rpx;
}
.imgs {
	width: 20rpx;
	height: 40rpx;
}
.p_text {
	display: table-cell;
	width: 620rpx;
	/* border: 1px red solid; */
	height: 60rpx;
	vertical-align: middle;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}

