/* pages/member/currency/index.wxss */
@import "./template/homestay/index.wxss"
@import "./template/catering/index.wxss"
.content{
	background-color: #f8f8f8;
}
.to-flex{
	display: flex;
	justify-content: space-between;
}
.w90{
	width: 90%;
	margin-left: 5%;
}
.p90{
	padding: 0 5%;
}
.header-bar{
	display: flex;
	position: absolute;
	top: 0;
	width: 40%;
	left: 50%;
	margin-left: -148rpx;
}
.header-bar view:first-child{
	border-top-left-radius: 10rpx;
	border-bottom-left-radius: 10rpx;
}
.header-bar view:last-child{
	border-top-right-radius: 10rpx;
	border-bottom-right-radius: 10rpx;
}
.header-bar view{
	width: 50%;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
	color: #595959;
	font-size: 34rpx;
	background-color: #f5f5f5;
	color: #2d2d2d;
	
	/* margin-left: ; */
}
.header-bar  .navbar-item-active{
	background-color: #49bdcc;
	color: #fff;
}
.content{
	margin-top: 60rpx;
	background-color: #f8f8f8;
}
.content-con{
	margin-top: 100rpx;
}

