/* pages/member/people/index.wxss */
.w90{
  width:90%;
  margin-left: 5%;
}
.p90{
  padding: 0 5%;
}
.to-flex{
  display: flex;
  justify-content: space-between;
}
.content{
  background-color: white;
  margin-top: 60rpx;
}
input{
  width: 73%;
  height: 50rpx;
  border-radius: 25rpx;
  background-color: white;
  padding-left: 100rpx;
}
.top-nav{
  margin-top: 20rpx;
}
.top-nav text{
  font-size: 30rpx
}
.content-item{
  background-color: white;
  padding: 30rpx 0;
  border-bottom: 1px solid #f5f5f5;
}
.content-item:last-child{
  border-bottom: none;
}
.item-left{
  width: 30%;
  line-height: 180rpx;
}
.item-right{
  width: 68%;
  font-size: 24rpx;
  color: #888888;
}
.item-left image{
  width: 80%;
  border-radius: 10rpx;
  vertical-align: middle;
}
.name{
  font-size: 32rpx;
  color: #333;
}
.icon-search{
  position: absolute;
  left: 50rpx;
  margin-top: 10rpx;
  color: #b4b4b4;
}
.item-right view{
  padding: 4rpx 0;
}